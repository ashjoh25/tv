<body>

    <div class="container">
        <div class="center">
            <h1>Show Ranker</h1>
        </div>
        <div class="centers">
            <ul id="desktop-nav" class="right hide-on-med-and-down">
                <% if (isLoggedIn) { %>
                    <li><a href="/logout" class="btn red">Logout <%=user.name%>!</a></li>                        
                <% } else { %>
                    <li><a href="/login" class="btn blue">Login</a></li>
                <% } %>
            </ul>
        </div>
        <div class="centering">
            <div class="section" style="text-align: center">
                <button onclick="location.href='/add_ranking'">Add New Show!</button>
            </div>
        </div>
        <div class="centerings">
            <div class="section" style="text-align: center">
                <button onclick="location.href='/add_ranking/all_rankings'">See Full List!</button>
            </div>
        </div>
    </div>
    <script> M.AutoInit();</script>
